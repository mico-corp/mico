{
  "name": "mico",
  "version-string": "1.0.4",
  "port-version": 1,
  "homepage": "www.mico-corp.com",
  "description": "Modular Interchangeable Computer prOgraming framework. Yet another zero coding programming library for developers and education..",
  "dependencies": [
    "boost-system",
    "boost-asio",
    "boost-program-options",
    "boost-exception",
    "boost-filesystem",
    "zlib",
    "yaml-cpp",
    "realsense2",
    "pangolin",
    {
      "name": "qt5",
      "default-features": false,
      "features": [
        "multimedia",
        "svg"
      ]
    },
    {
      "name": "vtk",
      "default-features": false
    },
    {
      "name": "pcl",
      "default-features": false,
      "features": [
        "visualization"
      ]
    },
    {
      "name": "opencv",
      "platform": "windows",
      "features": [
        "contrib",
        "cuda",
        "cudnn",
        "dnn"
      ]
    },
    {
      "name": "opencv",
      "platform": "linux",
      "features": [
        "contrib"
      ]
    },
    {
      "name":"dlfcn-win32",
    "platform": "windows"
    }
  ]
}